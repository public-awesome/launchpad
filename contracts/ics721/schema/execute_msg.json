{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ExecuteMsg",
  "oneOf": [
    {
      "type": "object",
      "required": [
        "receive"
      ],
      "properties": {
        "receive": {
          "$ref": "#/definitions/Cw721ReceiveMsg"
        }
      },
      "additionalProperties": false
    },
    {
      "type": "object",
      "required": [
        "transfer"
      ],
      "properties": {
        "transfer": {
          "$ref": "#/definitions/TransferMsg"
        }
      },
      "additionalProperties": false
    }
  ],
  "definitions": {
    "Binary": {
      "description": "Binary is a wrapper around Vec<u8> to add base64 de/serialization with serde. It also adds some helper methods to help encode inline.\n\nThis is only needed as serde-json-{core,wasm} has a horrible encoding for Vec<u8>",
      "type": "string"
    },
    "Cw721ReceiveMsg": {
      "description": "Cw721ReceiveMsg should be de/serialized under `Receive()` variant in a ExecuteMsg",
      "type": "object",
      "required": [
        "msg",
        "sender",
        "token_id"
      ],
      "properties": {
        "msg": {
          "$ref": "#/definitions/Binary"
        },
        "sender": {
          "type": "string"
        },
        "token_id": {
          "type": "string"
        }
      }
    },
    "TransferMsg": {
      "type": "object",
      "required": [
        "channel",
        "class_id",
        "remote_address",
        "token_ids",
        "token_uris"
      ],
      "properties": {
        "channel": {
          "type": "string"
        },
        "class_id": {
          "type": "string"
        },
        "class_uri": {
          "type": [
            "string",
            "null"
          ]
        },
        "remote_address": {
          "type": "string"
        },
        "timeout": {
          "type": [
            "integer",
            "null"
          ],
          "format": "uint64",
          "minimum": 0.0
        },
        "token_ids": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "token_uris": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      }
    }
  }
}
